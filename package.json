    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^5.7.0",
    "mongoose": "^7.4.1",
    "multer": "^1.4.5-lts.1"
  },
  "devDependencies": {
    "@google-cloud/functions-framework": "^3.3.0",
    "@types/bcryptjs": "^2.4.3",
    "@types/cors": "^2.8.13",
